// Seção de cabeçalho:
ALGORITMO "Exercicio01"

// Seção de Variaveis:
var lado : inteiro
    base : inteiro
    area : inteiro

INICIO

// Seção de Entrada de dados:
   EscrevaL("Qual o lado?")
   Leia (lado)

   EscrevaL("Qual a base?")
   Leia (base)

// Seção de procssamento:
   area := lado * base
   

// Seção de Saída:
   
   EscrevaL("Area Calulada:", area)

FIMALGORITMO




